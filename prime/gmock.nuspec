<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
    <metadata>
        <!-- The identifier that must be unique within the hosting gallery -->
        <id>gmock</id>

        <!-- The package version number that is used when resolving dependencies -->
        <version>1.10.0</version>

        <!-- Authors contain text that appears directly on the gallery -->
        <authors>lchavarr</authors>

        <!-- 
            Owners are typically nuget.org identities that allow gallery
            users to easily find other packages by the same owners.  
        -->
        <owners>lchavarr</owners>
        
         <!-- Project URL provides a link for the gallery -->
        <projectUrl>https://github.com/google/googletest</projectUrl>

        <!-- 
            The description can be used in package manager UI. Note that the
            nuget.org gallery uses information you add in the portal. 
        -->
        <description>Package contaning the binary compilation of GoolgleMock for Prime's requirements.</description>

        <!-- Tags appear in the gallery and can be used for tag searches -->
        <tags>EVG Evergreen Prime native Native GMock GoogleMock GoogleTest</tags>
        <language>C++</language>
    </metadata>

    <!-- A readme.txt to display when the package is installed -->
    <files>
        <!-- <file src="readme.txt" target="" /> -->
        <file src="TOS36\lib\Release\gmock.lib" target="lib\TOS36\Release\" />
        <file src="TOS37\lib\Release\gmock.lib" target="lib\TOS37\Release\" />
        <file src="TOS38\lib\Release\gmock.lib" target="lib\TOS38\Release\" />
        <file src="googlemock\include\gmock\**" target="include\gmock" />
        <file src="googletest\include\gtest\**" target="include\gtest" />
    </files>
</package>
